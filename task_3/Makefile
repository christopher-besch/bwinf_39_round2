# Alternative GNU Make workspace makefile autogenerated by Premake

ifndef config
  config=debug
endif

ifndef verbose
  SILENT = @
endif

ifeq ($(config),debug)
  task_3_config = debug

else ifeq ($(config),release)
  task_3_config = release

else
  $(error "invalid configuration $(config)")
endif

PROJECTS := task_3

.PHONY: all clean help $(PROJECTS) 

all: $(PROJECTS)

task_3:
ifneq (,$(task_3_config))
	@echo "==== Building task_3 ($(task_3_config)) ===="
	@${MAKE} --no-print-directory -C . -f task_3.make config=$(task_3_config)
endif

clean:
	@${MAKE} --no-print-directory -C . -f task_3.make clean

help:
	@echo "Usage: make [config=name] [target]"
	@echo ""
	@echo "CONFIGURATIONS:"
	@echo "  debug"
	@echo "  release"
	@echo ""
	@echo "TARGETS:"
	@echo "   all (default)"
	@echo "   clean"
	@echo "   task_3"
	@echo ""
	@echo "For more information, see https://github.com/premake/premake-core/wiki"